	<style>
		        button, input,select, textarea {
            border-radius:0px !important;
        }
        .collapsor {
            position: absolute;
            top:10px;
            right:10px;
            font-size:15px;
            font-weight: bold;
            padding:2px;
            cursor:pointer;
        }
		.navbar-brand {
			font-size:14px !important;
			margin-right:0px !important;
			padding:0 5px;
		}

		.navbar-brand:hover {
			cursor:pointer;
			color: #67a1d4 !important;
		}
		
		.blink_me {
			background: white;
			animation: blinker 5s linear infinite;
			padding: 5px;
			font-weight: bold;
			border-radius: 20px;
		}

		@keyframes blinker {
		50% {
			opacity: 0;
		}
		}

		table > tbody > tr:nth-of-type(odd) {
			background-color: #f8f8f8 ;
		}
	
		body{
			font-family: monospace;
			font-size:12px !important;
		}

		input,select, button {
			font-size:12px !important;
		}
	
	
	
	
		.dataValue {
			font-weight:bold;
			color:#cb3837;
		}
		
		.nodeId {
			font-weight:bold;
			color: #294f6f;
		}
		
		.dateValue {
			font-weight:bold;
			text-align: center;
		}
		
		.counter
		{
			font-weight:bold;
		}
	
		.mess-panel{
			padding:10px;
			background: #f8f8f8;
			list-style: none;
			margin:15px 5px;
			border-radius:10px;
		}
	
	
		.mess-panel h4{
			padding:10px;
			border-bottom: 3px solid #cb3837;
		}
	
		.tab-header{
			font-weight: bold;
			background: #67a1d4;
			color:#FFF;
		}
	
